{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@registry/registry/(.*)": "<rootDir>/../libs/registry/src/$1",
    "@registry/registry": "<rootDir>/../libs/registry/src"
  }
}